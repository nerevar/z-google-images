{
    "name": "z-google-images",
    "version": "0.0.12",
    "description": "Replace links from Google Images to Yandex Images",
    "content_scripts": [{
        "matches": [
            "http://www.google.com/*",
            "https://www.google.com/*"
        ],
        "js": ["contentscript.js"],
        "run_at": "document_end"
    }],
    "icons": {
        "48": "images/images-favicon-48x48.png"
    },
    "manifest_version": 2
}
