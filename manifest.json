{
    "name": "z-google-images",
    "version": "0.0.14",
    "description": "Replace links from Google Images to Yandex Images",
    "content_scripts": [{
        "matches": [
            "*://*.google.com/*",
            "*://*.google.ru/*"
        ],
        "js": ["contentscript.js"],
        "run_at": "document_end"
    }],
    "icons": {
        "48": "images/images-favicon-48x48.png"
    },
    "manifest_version": 2,
    "update_url": "https://github.com/nerevar/z-google-images/raw/releases/update_manifest.xml"
}
